"""
Simple working config for tenant parser
"""

import os

class Settings:
    def __init__(self):
        self.grpc_port = int(os.getenv("GRPC_PORT", "5012"))
        self.max_workers = int(os.getenv("MAX_WORKERS", "10"))
        self.otlp_endpoint = os.getenv("OTLP_ENDPOINT")
        self.telemetry_enabled = True
        self.service_name = "tenant_parser"

def get_settings():
    """Get settings - simple working version"""
    return Settings()
